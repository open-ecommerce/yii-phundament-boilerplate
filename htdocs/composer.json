{
    "name": "oecms/app",
    "description": "proof of concept and starting point for development with yii2 and phundament",
    "keywords": [
        "cms",
        "organizations",
        "application",
        "charities",
        "open-ecommerce",
        "open-ecommerce.org",
        "phundament"
    ],
    "homepage": "http://www.helptext.uk/",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "open-ecommerce.org",
            "email": "info@open-ecommerce.org",
            "homepage": "http://www.open-ecommerce.org"
        }
    ],
    "support": {
        "issues": "https://github.com/open-ecommerce/helptext/issues",
        "source": "https://github.com/open-ecommerce/helptext"
    },
    "repositories": [
            {
                "type": "vcs",
                "url": "https://github.com/open-ecommerce/yii2-backuprestore"
            }
    ],
  "minimum-stability": "stable",
  "require": {
    "dmstr/yii2-cms-metapackage": "*",
    "vlucas/phpdotenv": "2.*",
    "yiisoft/yii2": "^2.0.7",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "2.*",
    "kartik-v/yii2-widgets": "*",
    "kartik-v/yii2-grid": "*",
    "kartik-v/yii2-widget-select2": "*",
    "kartik-v/yii2-mpdf": "*",
    "kartik-v/yii2-helpers": "dev-master",
    "kartik-v/yii2-datecontrol": "dev-master",
    "kartik-v/yii2-export": "@dev",
    "kartik-v/yii2-detail-view": "@dev",
    "kartik-v/dependent-dropdown": "@dev",
    "kartik-v/yii2-editable": "*",
    "spanjeta/yii2-backup": "*",
    "open-ecommerce/yii2-backuprestore": "dev-master",
    "bower-asset/font-awesome": "^4.5",
    "bower-asset/bootstrap-sass": "3.3.6",
    "bower-asset/motion-ui": "^1.2.2",
    "bower-asset/sass-rem": "^1.2",
    "bower-asset/what-input": "^1.1",
    "bower-asset/admin-lte": "*",
    "cinghie/yii2-user-extended": "*",
    "filipajdacic/yii2-twilio": "dev-master",
    "twilio/sdk": "^4.10",
    "dmstr/yii2-adminlte-asset": "2.*",
    "borales/yii2-phone-input": "*",
    "2amigos/yii2-chartjs-widget" : "~2.0",
    "c006/yii2-migration-utility": "dev-master",
    "yii2mod/yii2-comments": "*",
    "yii2mod/yii2-cms": "*",
    "2amigos/yii2-translateable-behavior": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "1.* | 2.*",
    "schmunk42/yii2-giiant": "0.9.0-beta2",
    "schmunk42/yii2-markdocs-module": "~0.3",
    "yiisoft/yii2-apidoc": "2.*",
    "yiisoft/yii2-codeception": "2.*",
    "yiisoft/yii2-coding-standards": "2.*",
    "yiisoft/yii2-debug": "2.*",
    "yiisoft/yii2-faker": "2.*",
    "yiisoft/yii2-gii": "2.*",
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "config": {
    "process-timeout": 1800
  }
}
